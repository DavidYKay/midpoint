<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:state_pressed="true" >
        <shape>
          <solid android:color="@color/hyperpublic_blue_pressed" />
            <corners
                android:radius="3dp" />
        </shape>
    </item>
	<item>
        <shape>
            <solid
              android:color="@color/hyperpublic_blue" />
            <corners
                android:radius="2dp" />
        </shape>
    </item>
</selector>
